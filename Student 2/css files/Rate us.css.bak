input.star { display: none; }

label.star {
  float: right;
  padding: 10px;
  font-size: 40px;
  color:;
  transition: all .2s;
}

input.star:checked ~ label.star:before {
  content: '\2605';
  color: yellow;
  transition: all .25s;
}

input.star-5:checked ~ label.star:before {
  color: #8000080;
  text-shadow: 0 0 20px #952;
}

input.star-1:checked ~ label.star:before { color: yellow; }

label.star:hover { transform: rotate(-15deg) scale(1.3); }

label.star:before {
  content: '\2605';
  color: white;
} 
